<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE data-dictionary SYSTEM "data-dictionary.dtd">

<data-dictionary name="adsf">

    <description> <![CDATA[OnPrem - Cloud Common Implementation tables]]>
    </description>

    <table name="ADSClassLoader">
        <description>
            <![CDATA[Common Implementation Classes]]>
        </description>
        <columns>
            <column name="MODULE_ID">
                <description>Module ID </description>
                <data-type>BIGINT</data-type>
                <uniquevalue-generation>
                    <generator-name>ADSClassLoader.MODULE_ID</generator-name>
                </uniquevalue-generation>
            </column>
            <column name="MODULE_NAME">
                <description>Module Name </description>
                <data-type>CHAR</data-type>
                <max-size>250</max-size>
                <nullable>false</nullable>
            </column>
            <column name="IMPL_CLASS">
                <description>Implementation Class </description>
                <data-type>CHAR</data-type>
                <max-size>500</max-size>
                <nullable>false</nullable>
            </column>
            <column name="DESCRIPTION">
                <description>The Description of the module</description>
                <data-type>CHAR</data-type>
                <max-size>250</max-size>
            </column>
        </columns>
        <primary-key name="ADSClassLoader_PK">
            <primary-key-column>MODULE_ID</primary-key-column>
        </primary-key>
    </table>
    <table name="ADSNotificationCenter">
        <description>
            <![CDATA[Contains Notification Center Entry Details]]>
        </description>
        <columns>
            <column name="NOTIFICATION_ID">
                <description>UNIQUE ID</description>
                <data-type>BIGINT</data-type>
                <uniquevalue-generation>
                    <generator-name>ADSNotificationCenter.NOTIFICATION_ID</generator-name>
                </uniquevalue-generation>
            </column>
            <column name="NOTIFICATION_NAME">
                <description>The name of the notification module</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
                <nullable>false</nullable>
            </column>
            <column name="IMPL_CLASS">
                <description>Conatins Handler class name of the notifiaction module</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
                <nullable>false</nullable>
            </column>
            <column name="IS_ENABLED">
                <description>This flag specifies whether the notification module is enabled</description>
                <data-type>BOOLEAN</data-type>
                <default-value>TRUE</default-value>
                <nullable>false</nullable>
            </column>
        </columns>
        <primary-key name="ADSNotificationCenter_PK">
            <primary-key-column>NOTIFICATION_ID</primary-key-column>
        </primary-key>
    </table>

	<table name="ADSCountries">
		<description>
			<![CDATA[ Holds countries List
			]]>
		</description>
		<columns>
			<column name="COUNTRY_ID">
				<description>Unique identifier</description>
				<data-type>BIGINT</data-type>
				<uniquevalue-generation>
					<generator-name>ADSCountries.COUNTRY_ID</generator-name>
				</uniquevalue-generation>
			</column>
			<column name="COUNTRY_CODE">
				<description>Countries ID</description>
				<data-type>NCHAR</data-type>
				<max-size>10</max-size>
				<nullable>false</nullable>
			</column>
			<column name="CO">
				<description>Country Name</description>
				<data-type>NCHAR</data-type>
				<max-size>250</max-size>
				<nullable>false</nullable>
			</column>
			<column name="C">
				<description>Country Abbrevation</description>
				<data-type>NCHAR</data-type>
				<max-size>10</max-size>
				<nullable>false</nullable>
			</column>
		</columns>
		<primary-key name="ADSCountries_PK">
			<primary-key-column>COUNTRY_ID</primary-key-column>
		</primary-key>
	</table>
	<table name="ADSCountryStates">
		<description>
			<![CDATA[ Holds countries List
			]]>
		</description>
		<columns>
			<column name="STATE_ID">
				<description>Unique identifier</description>
				<data-type>BIGINT</data-type>
				<nullable>false</nullable>
				<uniquevalue-generation>
					<generator-name>ADSCountryStates.STATE_ID</generator-name>
				</uniquevalue-generation>
			</column>
			<column name="COUNTRY_ID">
				<description>Identifier of the country</description>
				<data-type>BIGINT</data-type>
				<nullable>false</nullable>
			</column>
			<column name="STATE_NAME">
				<description>State Name</description>
				<data-type>NCHAR</data-type>
				<max-size>250</max-size>
				<nullable>false</nullable>
			</column>
			<column name="STATE_CODE">
				<description>State Codes</description>
				<data-type>NCHAR</data-type>
				<max-size>10</max-size>
				<nullable>false</nullable>
			</column>
		</columns>
		<primary-key name="ADSCountryStates_PK">
			<primary-key-column>STATE_ID</primary-key-column>
		</primary-key>
		<foreign-keys>
			<foreign-key name="ADSCountryStates_FK" reference-table-name="ADSCountries" isbidirectional="false">
				<fk-columns>
					<fk-column>
						<fk-local-column>COUNTRY_ID</fk-local-column>
						<fk-reference-column>COUNTRY_ID</fk-reference-column>
					</fk-column>
				</fk-columns>
			</foreign-key>
		</foreign-keys>
	</table>
    <table name="ADSCache">
        <columns>
            <column name="UNIQUE_ID">
                <data-type>BIGINT</data-type>
                <nullable>false</nullable>
                <unique>true</unique>
                <uniquevalue-generation>
                    <generator-name>ADSCache.UNIQUE_ID</generator-name>
                </uniquevalue-generation>
            </column>
            <column name="CACHE_KEY">
                <data-type>CHAR</data-type>
                <max-size>250</max-size>
                <nullable>false</nullable>
            </column>
            <column name="CACHE_VALUE">
                <data-type>CHAR</data-type>
                <max-size>-1</max-size>
            </column>
        </columns>
        <primary-key name="ADSCache_PK">
            <primary-key-column>UNIQUE_ID</primary-key-column>
        </primary-key>
    </table>

    <table name="ADSSystemDefaults">
        <description><![CDATA[Contains the default Personalize Settings details ]]></description>
        <columns>
            <column name="UNIQUE_ID">
                <description>Unique value for each parameter</description>
                <data-type>BIGINT</data-type>
                <uniquevalue-generation>
                    <generator-name>ADSSystemDefaults.UNIQUE_ID</generator-name>
                </uniquevalue-generation>
            </column>
            <column name="BROWSER_TITLE">
                <description>Title to display in the browser</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
            </column>
            <column name="LOGO_PATH">
                <description>Path of the logo located</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
            </column>
            <column name="LOGO_WIDTH">
                <description>Width of the logo to be displayed</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
            </column>
            <column name="LOGO_HEIGHT">
                <description>Height of the logo to be displayed</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
            </column>
            <column name="FAVICON_PATH">
                <description>Path of the favicon located</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
            </column>
            <column name="THEME_COLOR">
                <description>Theme color of the product</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
            </column>
            <column name="TIME_ZONE">
                <description>Time zone to be used</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
            </column>
            <column name="DATE_TIME_FORMAT">
                <description>Time and Date format to be displayed</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
            </column>
            <column name="ENABLE_FORGOT_PWD">
                <description>Boolean to show the forgot password link</description>
                <data-type>BOOLEAN</data-type>
                <max-size>250</max-size>
                <default-value>true</default-value>
            </column>
        </columns>
        <primary-key name="ADSSystemDefaults_PK">
            <primary-key-column>UNIQUE_ID</primary-key-column>
        </primary-key>
    </table>
    <!--Locale-->
    <table name="ADSLocaleDetails">
        <description><![CDATA[I18N Local Details]]></description>
        <columns>
            <column name="LOCALE_ID">
                <description>A unique locale id for each country languages</description>
                <data-type>BIGINT</data-type>
                <uniquevalue-generation>
                    <generator-name>ADSLocaleDetails.LOCALE_ID</generator-name>
                </uniquevalue-generation>
            </column>
            <column name="LANGUAGE">
                <description>Locale Language</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
                <nullable>false</nullable>
            </column>
            <column name="COUNTRY">
                <description>Locale Country</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
            </column>
            <column name="DISPLAY_NAME">
                <description>Locale Display Name</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
                <nullable>false</nullable>
            </column>
        </columns>
        <primary-key name="ADSLocaleDetails_PK">
            <primary-key-column>LOCALE_ID</primary-key-column>
        </primary-key>
    </table>

    <!--Products-->
    <table name="ADSProducts">
        <description>
            <![CDATA[Supported AD Products]]>
        </description>
        <columns>
            <column name="PRODUCT_ID">
                <description>AD Product Unique Id</description>
                <data-type>BIGINT</data-type>
                <uniquevalue-generation>
                    <generator-name>ADSProducts.PRODUCT_ID</generator-name>
                </uniquevalue-generation>
            </column>
            <column name="PRODUCT_NAME">
                <description>AD Product's Full Name e.g ManageEngine ADManager Plus</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
                <nullable>false</nullable>
                <unique>true</unique>
            </column>
            <column name="TAG_NAME">
                <description>Product abbreviation</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
            </column>
            <column name="DISPLAY_NAME">
                <description>AD Product's Display Name e.g ADManager Plus</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
                <nullable>false</nullable>
            </column>
            <column name="ICON_STYLE">
                <description>Icon Style</description>
                <data-type>NCHAR</data-type>
                <max-size>100</max-size>
            </column>
            <column name="IS_CUSTOM_APP">
                <description>User can add custom application in the apps pane</description>
                <data-type>BOOLEAN</data-type>
                <default-value>false</default-value>
                <nullable>false</nullable>
            </column>
            <column name="IS_PRIMARY_PRODUCT">
                <description>Check for Primary Product</description>
                <data-type>BOOLEAN</data-type>
                <default-value>true</default-value>
                <nullable>false</nullable>
            </column>
            <column name="PRIORITY_ID">
                <description>Priority in which order it should be showed</description>
                <data-type>BIGINT</data-type>
                <default-value>0</default-value>
            </column>
            <column name="IS_ENABLED">
                <description>Components shown or hide in dashboard will be decided based on this column</description>
                <data-type>BOOLEAN</data-type>
                <default-value>true</default-value>
                <nullable>false</nullable>
            </column>
            <column name="COMPONENT_NAME">
                <description>AD Product's License Component Name e.g AD Auditing</description>
                <data-type>NCHAR</data-type>
                <max-size>250</max-size>
            </column>
            <column name="ICON_DISPLAY_NAME">
                <description>Icon text</description>
                <data-type>NCHAR</data-type>
                <max-size>100</max-size>
            </column>
        </columns>
        <primary-key name="ADSProducts_PK">
            <primary-key-column>PRODUCT_ID</primary-key-column>
        </primary-key>
    </table>
    <table name="ADSProductParams">
        <description>
            <![CDATA[Maintaining the Product params details]]>
        </description>
        <columns>
            <column name="PRODUCT_ID">
                <description>AD Product Id.</description>
                <data-type>BIGINT</data-type>
                <nullable>false</nullable>
            </column>
            <column name="PARAM_NAME">
                <description>Key name of the parameter.</description>
                <data-type>NCHAR</data-type>
                <max-size>100</max-size>
                <nullable>false</nullable>
            </column>
            <column name="PARAM_VALUE">
                <description>Value of the parameter.</description>
                <data-type>NCHAR</data-type>
                <max-size>4000</max-size>
            </column>
        </columns>
        <primary-key name="ADSProductParams_PK">
            <primary-key-column>PRODUCT_ID</primary-key-column>
            <primary-key-column>PARAM_NAME</primary-key-column>
        </primary-key>
        <foreign-keys>
            <foreign-key name="ADSProductParams_FK" reference-table-name="ADSProducts">
                <fk-columns>
                    <fk-column>
                        <fk-local-column>PRODUCT_ID</fk-local-column>
                        <fk-reference-column>PRODUCT_ID</fk-reference-column>
                    </fk-column>
                </fk-columns>
                <fk-constraints>ON-DELETE-CASCADE</fk-constraints>
            </foreign-key>
        </foreign-keys>
    </table>


    <!-- Response status sent from ajax calls starts-->

	<table name="ADSResponseStatusCodes">
		<description>
			<![CDATA[contains Status code and details of localerrorcodes]]>
		</description>
		<columns>
			<column name="UNIQUE_ID">
				<data-type>BIGINT</data-type>
				<nullable>false</nullable>
				<uniquevalue-generation>
					<generator-name>ADSResponseStatusCodes.UNIQUE_ID</generator-name>
				</uniquevalue-generation>
			</column>
			<column name="STATUS_CODE">
				<data-type>BIGINT</data-type>
				<max-size>50</max-size>
				<nullable>false</nullable>
				<unique>true</unique>
			</column>
			<column name="DESCRIPTION">
				<data-type>NCHAR</data-type>
				<max-size>255</max-size>
				<nullable>false</nullable>
				<unique>false</unique>
			</column>
			<column name="DISPLAYTEXT">
				<data-type>CHAR</data-type>
				<max-size>255</max-size>
				<nullable>false</nullable>
				<unique>false</unique>
			</column>
			<column name="TROUBLESHOOTING_LINK">
				<data-type>CHAR</data-type>
				<max-size>-1</max-size>
				<nullable>true</nullable>
				<unique>false</unique>
			</column>
		</columns>
		<primary-key name="ADSResponseStatusCodes_PK">
			<primary-key-column>UNIQUE_ID</primary-key-column>
		</primary-key>
	</table>


