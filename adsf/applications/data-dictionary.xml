<table name="ADSIAMApplications">
		<description>
			<![CDATA[
			To specify the Other Accounts Password to be synced with the AD user Accounts
			]]>
		</description>
		<columns>
			<column name="APP_ID">
				<description>Unique ID</description>	 
                <data-type>BIGINT</data-type>	 
				<uniquevalue-generation>
					<generator-name>ADSIAMApplications.APP_ID</generator-name>
				</uniquevalue-generation>
			</column>
			<column name="CATEGORY_ID">
				<description>Unique ID.</description>
				<data-type>BIGINT</data-type>
				<unique>false</unique>
			</column>
			<column name="APP_NAME">
				<description>SYNCAccountName.</description>
				<data-type>NCHAR</data-type>
				<max-size>250</max-size>
				<unique>true</unique>
			</column>
			<column name="DISPLAY_NAME">
				<description>Display name of the provider.</description>
				<data-type>NCHAR</data-type>
				<max-size>250</max-size>
				<unique>false</unique>
			</column>
			<column name="ICON_STYLE">
				<description>Name of the image icon for the provider.</description>
				<data-type>NCHAR</data-type>
				<max-size>250</max-size>
				<unique>false</unique>
			</column>
			<column name="PRIORITY">
				<description>To differenciate colud and host providers</description>
				<data-type>BIGINT</data-type>
				<nullable>false</nullable>
			</column>
			<column name="SP_BIG_ICON">
				<description>Name of the image icon for the provider.</description>
				<data-type>NCHAR</data-type>
				<max-size>-1</max-size>
				<nullable>true</nullable>
			</column>
			<column name="SP_SMALL_ICON">
				<description>Name of the image icon for the provider.</description>
				<data-type>NCHAR</data-type>
				<max-size>-1</max-size>
				<nullable>true</nullable>
			</column>
		</columns>
		<primary-key name="ADSIAMApplications_PK">
			<primary-key-column>APP_ID</primary-key-column>
		</primary-key>
		<foreign-keys>
			<foreign-key reference-table-name="ADSIAMCategory" name="ADSIAMApplications_FK">
				<fk-columns>
					<fk-column>
						<fk-local-column>CATEGORY_ID</fk-local-column>
						<fk-reference-column>CATEGORY_ID</fk-reference-column>
					</fk-column>
				</fk-columns>
				<fk-constraints>ON-DELETE-CASCADE</fk-constraints>
			</foreign-key>
		</foreign-keys>
	</table>


